        <aside class="sidebar">
            <div class="logo ms-5 ">Admin Dashboard</div>
            <br> <br>
            <ul class="">
                <li><a href="/admin/dashboard" class="active"><i class="fas fa-home"></i> Dashboard</a></li>
                <li><a href="/admin/userlist" class="active"><i class="fa-solid fa-users"></i> Users</a></li>
